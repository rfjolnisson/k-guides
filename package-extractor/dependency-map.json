  "extraction_notes": {
    "shared_objects": [
      "Location__c: Geographic locations are org-wide, shared across packages",
      "Account: Suppliers are org-wide",
      "CommissionGroup__c: May be shared across packages",
      "DiscountGroup__c: May be shared across packages"
    ],
    "deployment_order": [
      "1. Location__c (no dependencies)",
      "2. Language__c (no dependencies)",
      "3. Account (Suppliers - may already exist)",
      "4. CommissionGroup__c, DiscountGroup__c, ProfitabilityGroup__c, TaxGroup__c (lookup dependencies - may not exist in export)",
      "5. PriceCategoryType__c (category type definitions)",
      "6. PaymentScheduleConfiguration__c",
      "7. PaymentScheduleRule__c (child of PaymentScheduleConfiguration)",
      "8. CancellationConfiguration__c",
      "9. CancellationRule__c (child of CancellationConfiguration)",
      "10. Package__c (depends on lookups above)",
      "11. PackageServiceLevelAssignment__c (depends on Package + ServiceLevel)",
      "12. PaymentScheduleAssignment__c (depends on Package + PaymentScheduleConfiguration)",
      "13. CancellationAssignment__c (depends on Package + CancellationConfiguration)",
      "14. PackageDay__c (depends on Package)",
      "15. PackageDayLocation__c (depends on PackageDay + Location)",
      "16. PackageInformation__c (depends on Package + Language)",
      "17. Component__c (depends on Package)",
      "18. Item__c (depends on Location, Account)",
      "19. PriceSeason__c (depends on Item - seasonal pricing)",
      "20. Item_Price__c (depends on Item, PriceSeason)",
      "21. Price_Category__c (depends on Item, PriceCategoryType)",
      "22. ItemPriceCategoryAssignment__c (depends on Item + Price_Category)",
      "23. AllotmentCategory__c (depends on Item)",
      "24. AllotmentDay__c (depends on AllotmentCategory)",
      "25. InventorySeasonStatus__c (depends on AllotmentCategory)",
      "26. ComponentOption__c (depends on Component, Item, Price_Category)",
      "27. ComponentPricelistAssignment__c (depends on Component)",
      "28. PackageDeparture__c (depends on Package)",
      "29. PackageDepartureAllotmentAssignment__c (depends on PackageDeparture + AllotmentCategory + AllotmentDay)",
      "30. ComponentOptionDepartureAssignment__c (depends on ComponentOption + PackageDeparture)",
      "31. PackageDepartureNoteAssignment__c (depends on PackageDeparture)"
    ],
    "circular_dependencies": "None identified in standard package configuration",
    "excluded_objects": [
      "Itinerary__c: Actual bookings (not part of package template)",
      "ItineraryItem__c: Booking line items (not part of package template)",
      "Passenger__c: Customer data (not part of package template)",
      "Transaction__c: Financial transactions (not part of package template)"
    ]
  }
